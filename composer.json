{
    "name": "smart-cms/kit",
    "description": "This is my package kit",
    "keywords": [
        "SmartCms",
        "laravel",
        "kit"
    ],
    "homepage": "https://github.com/smart-cms/kit",
    "support": {
        "issues": "https://github.com/smart-cms/kit/issues",
        "source": "https://github.com/smart-cms/kit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "maxboyko",
            "email": "maxboiko@divotek.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "laravel-notification-channels/telegram": "^6.0",
        "lorisleiva/laravel-actions": "^2.8",
        "smart-cms/forms": "^1.0",
        "smart-cms/lang": "^1.0",
        "smart-cms/menu": "^1.0",
        "smart-cms/model-translate": "^1.0",
        "smart-cms/panel-settings": "^1.0",
        "smart-cms/panel-translate": "^1.0",
        "smart-cms/seo": "^1.0",
        "smart-cms/support": "^1.0",
        "smart-cms/template-builder": "^1.0",
        "smart-cms/theme": "^1.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-translatable": "^6.11"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SmartCms\\Kit\\": "src/",
            "SmartCms\\Kit\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SmartCms\\Kit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse --memory-limit 1G -c phpstan.neon.dist ",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartCms\\Kit\\KitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
